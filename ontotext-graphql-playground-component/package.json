{
  "name": "ontotext-graphql-playground-component",
  "version": "0.0.6-TR4",
  "description": "Ontotext GraphQL Playground Component is a reusable web component built with StencilJS, designed to provide an intuitive and interactive interface for exploring GraphQL APIs. This framework-agnostic component integrates with web frameworks like Angular, React, and more. It leverages GraphiQL and includes custom enhancements tailored for Ontotextâ€™s GraphDB Workbench, providing GraphQL query execution and visualization.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ontotext-graphql-playground-component/ontotext-graphql-playground-component.esm.js",
  "exports": {
    ".": {
      "import": "./dist/ontotext-graphql-playground-component/ontotext-graphql-playground-component.esm.js",
      "require": "./dist/ontotext-graphql-playground-component/ontotext-graphql-playground-component.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "prebuild": "npm run copy-files",
    "build": "stencil build",
    "prestart": "npm run copy-files",
    "start": "stencil build --dev --watch --serve",
    "copy-files": "cd scripts && ./copy-files.sh",
    "test": "stencil test --spec --files src/**/*.spec.ts",
    "test.watch": "stencil test --spec --files src/**/*.spec.ts --watchAll",
    "generate": "stencil generate",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "cy:open": "cypress open",
    "cy:run": "concurrently --kill-others --success first \"npm run start\" \"wait-on http://localhost:3333 && cypress run\""
  },
  "dependencies": {
    "graphql": "^16.9.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@stencil-community/eslint-plugin": "^0.9.0",
    "@stencil/angular-output-target": "^0.10.0",
    "@stencil/core": "^4.22.3",
    "@stencil/sass": "^3.0.12",
    "@types/jest": "^29.5.6",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "concurrently": "^9.1.0",
    "cypress": "^13.16.1",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rxjs": "^7.8.1",
    "typescript": "^5.7.2",
    "wait-on": "^8.0.1"
  },
  "license": "MIT"
}
